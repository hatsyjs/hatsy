{
  "name": "@hatsy/hatsy",
  "version": "1.6.0",
  "description": "Asynchronous TypeScript-friendly HTTP server for Node.js",
  "keywords": [
    "api",
    "http",
    "http-server",
    "rest",
    "restful",
    "router",
    "web",
    "web-server",
    "typescript"
  ],
  "homepage": "https://github.com/hatsyjs/hatsy",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/hatsy.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/hatsy/issues"
  },
  "main": "dist/hatsy.js",
  "module": "dist/hatsy.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hatsy.mjs",
      "require": "./dist/hatsy.js"
    },
    "./core": {
      "import": "./dist/hatsy.core.mjs",
      "require": "./dist/hatsy.core.js"
    },
    "./testing": {
      "import": "./dist/hatsy.testing.mjs",
      "require": "./dist/hatsy.testing.js"
    }
  },
  "peerDependencies": {
    "jest": "^26.0.1"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    }
  },
  "dependencies": {
    "@hatsy/http-header-value": "^3.4.0",
    "@proc7ts/a-iterable": "^2.8.0",
    "@proc7ts/call-thru": "^4.1.0",
    "@proc7ts/primitives": "^1.2.0"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@proc7ts/eslint-config": "^2.0.4",
    "@proc7ts/rollup-helpers": "^1.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.54",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "codecov": "^3.7.2",
    "eslint": "^7.7.0",
    "eslint-plugin-jest": "^23.20.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "rollup": "^2.26.6",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "run-z": "^1.0.0-dev.9",
    "shx": "^0.3.2",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.18.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+lint,+test,+doc"
  }
}
