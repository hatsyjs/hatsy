{
  "name": "@hatsy/hatsy",
  "version": "2.0.0",
  "description": "Asynchronous TypeScript-friendly HTTP server for Node.js",
  "keywords": [
    "api",
    "http",
    "http-server",
    "rest",
    "restful",
    "router",
    "web",
    "web-server",
    "typescript"
  ],
  "homepage": "https://github.com/hatsyjs/hatsy",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/hatsy.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/hatsy/issues"
  },
  "type": "module",
  "main": "dist/hatsy.cjs",
  "module": "dist/hatsy.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hatsy.js",
      "require": "./dist/hatsy.cjs"
    },
    "./core": {
      "import": "./dist/hatsy.core.js",
      "require": "./dist/hatsy.core.cjs"
    },
    "./package.json": "./package.json",
    "./testing": {
      "import": "./dist/hatsy.testing.js",
      "require": "./dist/hatsy.testing.cjs"
    }
  },
  "peerDependencies": {
    "jest": "^26.0.1"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    }
  },
  "dependencies": {
    "@hatsy/hten": "^1.1.0",
    "@hatsy/http-header-value": "^3.5.0",
    "@proc7ts/primitives": "^1.4.0"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@proc7ts/eslint-config": "^2.0.5",
    "@proc7ts/rollup-helpers": "^1.1.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codecov": "^3.8.1",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.33.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "run-z": "^1.6.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
